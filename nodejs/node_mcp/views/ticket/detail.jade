    body
        input(id="entityId", type="hidden", value=entity.id)
        #tWin.container
        .bodyAdminDiv
            .clearfix(style="margin-top:8px")
                .vs_grid_plain(style="width:100px;text-align:right;") id:
                .vs_grid_plain(style="margin-left:8px;")=entity.id
            .clearfix(style="margin-top:8px")
                .vs_grid_plain(style="width:100px;text-align:right;") 外部id:
                .vs_grid_plain(style="margin-left:8px;")=entity.outerId
            .clearfix(style="margin-top:8px")
                .vs_grid_plain(style="width:100px;text-align:right;") 游戏:
                .vs_grid_plain(style="margin-left:8px;")=entity.gameCode
            .clearfix(style="margin-top:8px")
                .vs_grid_plain(style="width:100px;text-align:right;") 期次:
                .vs_grid_plain(style="margin-left:8px;")=entity.termCode
            .clearfix(style="margin-top:8px")
                .vs_grid_plain(style="width:100px;text-align:right;") 创建时间:
                .vs_grid_plain(style="margin-left:8px;")=entity.createTime
            .clearfix(style="margin-top:8px")
                .vs_grid_plain(style="width:100px;text-align:right;") 出票状态:
                .vs_grid_plain(style="margin-left:8px;")=entity.printStatus.des
            .clearfix(style="margin-top:8px")
                .vs_grid_plain(style="width:100px;text-align:right;") 主状态:
                .vs_grid_plain(style="margin-left:8px;")=entity.status.des
            .clearfix(style="margin-top:8px")
                .vs_grid_plain(style="width:100px;text-align:right;") 玩法:
                .vs_grid_plain(style="margin-left:8px;")=entity.pTypeDetail.name
            .clearfix(style="margin-top:8px")
                .vs_grid_plain(style="width:100px;text-align:right;") 投注方式:
                .vs_grid_plain(style="margin-left:8px;")=entity.bTypeDetail.name
            .clearfix(style="margin-top:8px")
                .vs_grid_plain(style="width:100px;text-align:right;") 倍数:
                .vs_grid_plain(style="margin-left:8px;")=entity.multiple
            .clearfix(style="margin-top:8px")
                .vs_grid_plain(style="width:100px;text-align:right;") 金额:
                .vs_grid_plain(style="margin-left:8px;")=entity.amount/100 + '(元)'
            .clearfix(style="margin-top:8px")
                .vs_grid_plain(style="width:100px;text-align:right;") 投注号码:
                .vs_grid_plain(style="margin-left:8px;")=entity.number
            .clearfix(style="margin-top:8px")
                .vs_grid_plain(style="width:100px;text-align:right;") (出票)返回号码:
                .vs_grid_plain(style="margin-left:8px;")=entity.rNumber
            .clearfix(style="margin-top:8px")
                .vs_grid_plain(style="width:100px;text-align:right;") 开奖号码:
                .vs_grid_plain(style="margin-left:8px;")=entity.dNumber
            .clearfix(style="margin-top:8px")
                .vs_grid_plain(style="width:100px;text-align:right;") 税前奖金:
                .vs_grid_plain(style="margin-left:8px;")=entity.bonusBeforeTax/100 + '(元)'
            .clearfix(style="margin-top:8px")
                .vs_grid_plain(style="width:100px;text-align:right;") 税后奖金:
                .vs_grid_plain(style="margin-left:8px;")=entity.bonus/100 + '(元)'
            .clearfix(style="margin-top:8px")
                .vs_grid_plain(style="width:100px;text-align:right;") 中奖详情:
                .vs_grid_plain(style="margin-left:8px;")
                    - each val in entity.bonusDetail
                        p='(' + val.gameGrade.des + ',' + val.count + '注,税前' + val.bonusBeforeTax/100 + '元,税后' + val.bonus/100 + '元)*' + entity.multiple + '倍'